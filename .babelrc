{
    "presets": ["react", "env", "es2015"],
    "env": {
		"development": {
			"plugins": [
				[
					"transform-runtime",
					{
						"helpers": false,
						"polyfill": false,
						"regenerator": true,
						"moduleName": "babel-runtime"
					}
				],
				["react-transform", {
					"transforms": [{
						"transform": "react-transform-hmr",
						
						"imports": ["react"],
						
						"locals": ["module"]
					}]
				}]
			]
		}
    }
  }
